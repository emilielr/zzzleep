<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zzzleep" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_z"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_z"
        android:supportsRtl="true"
        android:theme="@style/Theme.Zzzleep" >
        <activity
            android:name=".ui.settings.RegisterActivity"
            android:exported="false"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.Zzzleep.NoActionBar" />

        <receiver
            android:name=".ui.settings.AlarmReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".MainActivity2"
            android:exported="false" />
        <activity android:name=".TimerActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>